# Configurações do RotaLivre Search API

# Servidor
HOST=0.0.0.0
PORT=8000
DEBUG=true

# APIs (opcional - para funcionalidades avançadas)
SERPAPI_KEY=your_serpapi_key_here
GOOGLE_MAPS_KEY=your_google_maps_key_here

# Banco de dados
DATABASE_URL=sqlite:///data/moto_weather.db

# Cache (opcional - para usar Redis)
REDIS_URL=redis://localhost:6379
CACHE_TTL=3600

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# Logs
LOG_LEVEL=info
